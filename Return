function sumNums(num1, num2) {  
    return num1+num2;
 }
 console.log(5)
 console.log(8)

 //funtion sumNums(num2,3)
 //return num2,3+ 3,5

 function sumNums(num1, num2) {//se declara la funcion que recibe dos parametros 
    var sum = num1 + num2;//declaramos la variable sum que es igual a num1+num2  
    console.log('sum is', sum);//imprimimos la variable sum 
    return sum;//retornamos la varuiable sum 
}

//Realizamos la invocacion de la funcion SUMNUMS 
var result = 
sumNums(2,3) + // 5
sumNums(3,
    sumNums(2,1) //3
    ) + //6
sumNums(
    sumNums(2,1),//3
    sumNums(2,3)//5
    );//8
console.log('result is', result);
//result es igual a 19
